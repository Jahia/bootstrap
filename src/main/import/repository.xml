<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <bootstrap j:dependencies="default siteSettings skins"
                 j:modulePriority="0"
                 j:moduleType="module"
                 j:title="bootstrap"
                 jcr:mixinTypes="jmix:hasExternalProviderExtension"
                 jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder">
                    <bootstrap jcr:primaryType="jnt:folder">
                        <css jcr:primaryType="jnt:folder">
                            <bootstrap.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap.css>
                            <bootstrap3.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap3.css>
                        </css>
                        <img jcr:primaryType="jnt:folder">
                            <glyphicons-halflings-white.png j:height="159"
                                                  j:width="469"
                                                  jcr:mixinTypes="jmix:image"
                                                  jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-white.png>
                            <glyphicons-halflings.png j:height="159"
                                            j:width="469"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings.png>
                        </img>
                        <fonts jcr:primaryType="jnt:folder">
                            <glyphicons-halflings-regular.svg jcr:mixinTypes="jmix:image" jcr:primaryType="jnt:file">
                                <jcr:content j:lastExtractionDate="2016-02-24T18:02:27.499+01:00"
                                  jcr:mimeType="image/svg+xml"
                                  jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-regular.svg>
                            <glyphicons-halflings-regular.ttf jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="application/x-font-ttf" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-regular.ttf>
                            <glyphicons-halflings-regular.eot jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="application/vnd.ms-fontobject"
                                  jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-regular.eot>
                            <glyphicons-halflings-regular.woff2 jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-regular.woff2>
                            <glyphicons-halflings-regular.woff jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="application/x-font-woff" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-regular.woff>
                        </fonts>
                    </bootstrap>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <bootstrap.png j:height="16"
                              j:width="16"
                              jcr:mixinTypes="jmix:image"
                              jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </bootstrap.png>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <site-settings-base j:view="siteSettings"
                                jcr:mixinTypes="jmix:useUILocale"
                                jcr:primaryType="jnt:template">
                        <bootstrapCustomization j:applyOn="jnt:virtualsite"
                                       j:defaultTemplate="false"
                                       j:hiddenTemplate="true"
                                       j:icon="/modules/bootstrap/templates/files/bootstrap.png"
                                       j:priority="100"
                                       j:requiredPermissionNames="adminBootstrapCustomization"
                                       j:titleKey="siteSettings.label.bootstrap"
                                       jcr:mixinTypes="jmix:rbTitle"
                                       jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <editLessVariables formview="advanced" jcr:primaryType="jnt:editLessVariables"/>
                            </pagecontent>
                        </bootstrapCustomization>
                    </site-settings-base>
                </templates>
                
            </bootstrap>
    </modules>
</content>
